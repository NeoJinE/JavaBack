package Ch06_class_sec10;

import java.util.Scanner;

public class Rectangle {

	//1.멤버변수선언
	private int Width;
	private int Height;
	
	//2.메서드1작성(매개변수로 가로,세로길이 매개변수)
	public void  setRectangle(Scanner sc) {
		System.out.println("가로길이: ");
		Width = sc.nextInt();
		System.out.println("세로길이: ");
		Height = sc.nextInt();
	
	}
		
	//3.메서드2작성(넓이 return)
	public int  getArea() {
		return Width * Height;
	}
	
	
	
	
	
	
	
	
	
	
	
}
