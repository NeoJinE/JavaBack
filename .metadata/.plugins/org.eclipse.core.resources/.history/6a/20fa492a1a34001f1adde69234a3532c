package sec06;

public class ProductMain {
	public static void main(String[] args) {
		//1.클래스 선언
		Product list = new Product();
		
		//2.상품정보입력 메서드 출력
		list.inputProduct();
		
		//3.학생정보출력 메서드 호출
		list.showProduct();
		
		//4.매출액 메서드 호출
		list.showSaleAmount();
		
		//5.재고액 메서드 호출
		list.showStockAmount();
		
	}
}
