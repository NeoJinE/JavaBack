package Ch06_class_sec11;

import java.util.Scanner;

public class SavingsMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		//1. 클래스 호출
		Savings List = new Savings();
		
		//2.메서드 호출
		List.setName();
		List.setBalance();
		List.getInterest();
		
		
		
		//3.출력메서드 호출
		List.showList(sc);
		sc.close();
	}

}
