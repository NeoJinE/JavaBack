package Ch07_inheritance.sec05;

public class Person {

	//1.멤버변수 선언
		//1.멤버변수선언
		private String name,ssn;
		 
		//2.(멤버메서드)생성자 선언
		
		public setPerson(String name, String ssn) {
			this.name = name;
			this.ssn = ssn;
		}

		//3.toString 작성
		@Override
		public String toString() {
			return name + ", " +ssn; 
		}
	}

	

