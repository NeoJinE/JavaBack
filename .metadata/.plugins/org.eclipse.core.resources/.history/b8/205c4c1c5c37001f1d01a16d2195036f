package Ch07_inheritance.sec07;

public class Automobile extends Car {

		//1.멤버변수 선언
		String autoMoblie;
		
		//2.생성자 초기화
		public void  setTrans () {
			setCar();
			System.out.println("기어변속");
			this.autoMoblie = sc.next();
		}

		//3.toString 출력문
		@Override
		public String toString() {
			return super.toString() + "기어변속: " + autoMoblie;
		}
		
		
		
		
		
}
