package Ch06_2.sec_23;

import java.util.Scanner;

public class BookMain {

	public static void main(String[] args) {
		// 도서 정보 출력
		Scanner sc = new Scanner(System.in);
		//1.클래스 호출
		Book info = new Book(null, null, null, 0);
		
		//2.생성자 호출
		
		System.out.println(info);
		
		//3.가격변경
		info.setPrice(0);
		
		//4.변경된 가격 출력
		System.out.println();
		System.out.println(info);
	}

}
