package Ch06_class_sec02;

public class Car {
	//동일 패키지내에서 접근 간으한 멤버필드 or 멤버변수를 생성 및 선언.
	//동일 패키지 접근 가능: default => 접근제한자를 생략
	 String carNo; //차량번호
	 String carName;//차종
	 String carMaker;//제조회사
	 int carYear;//연식
	 int carCC;//배기량
	
	//멤버 메서드
	//차량 번호만 출력
	 public void showCarInfo() {
		 System.out.println("차량번호: "+carNo);
	 }
	 
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
