package Ch4;

import java.util.Scanner;

public class Ch04_21_ForEx3 {

	public static void main(String[] args) {
		// 구구단의 단 수를 입력 받아서 해당 단을 출력하는 프로그램 작성

		Scanner sc = new Scanner(System.in);
		//1. for문을 이용한 코드
		int num1,num2,num3,num4,num5,num6,num7,num8,num9,num10;
		System.out.println("숫자1입력");
		num1 = sc.nextInt();
		System.out.println("숫자2입력");
		num2 = sc.nextInt();
		System.out.println("숫자3입력");
		num3 = sc.nextInt();
		System.out.println("숫자4입력");
		num4 = sc.nextInt();
		System.out.println("숫자5입력");
		num5 = sc.nextInt();
		System.out.println("숫자6입력");
		num6 = sc.nextInt();
		System.out.println("숫자7입력");
		num7 = sc.nextInt();
		System.out.println("숫자8입력");
		num8 = sc.nextInt();
		System.out.println("숫자9입력");
		num9 = sc.nextInt();
		System.out.println("숫자10입력");
		num10 = sc.nextInt();
		
		
		
		
		
		
		
		
		
		
		
		
		sc.close();
		
	}

}
