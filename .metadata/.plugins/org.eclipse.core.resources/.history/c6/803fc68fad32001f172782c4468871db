package Ch05_array;

public class Ch05_19_Ex2 {

	public static void main(String[] args) {
		// 2차원 배열을 사용하여 각 과목 점수와 총점, 평균을 출력하는 프로그램
		//1.배열선언
		String[]Sub = {"번호","국어","영어","수학","총점","평균"};
		double[][]Score= {
				{1,100,100,100,300,100.0},
				{2,20,20,20,60,20.0},
				{3,30,30,30,90,30.0},
				{4,40,40,40,120,40.0},
				{5,50,50,50,150,50.0},
				{6,20,20,20,60,20.0}	
		};
		for(int i=0; i<6;i++) {
			System.out.print(Sub[i]+"    ");		
		}
		System.out.println("\n===============================================");
		//2.번호~평균배열작성
		//3.각 번호, 점수 작성
		//4.총점작성
		//5.출력문
	}

}
