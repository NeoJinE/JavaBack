package Practice;

import java.util.Scanner;

public class Practice1 {

	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        int[] num = new int[10]; // 10개의 정수를 저장할 배열 생성

	        System.out.println("숫자 1~10을 입력하세요:");

	        // 10개의 정수를 입력받아 배열에 저장
	        for (int i = 0; i < 10; i++) {
	            System.out.println("숫자 " + (i + 1) + " 입력: ");
	            num[i] = sc.nextInt();
	        }

	        // 배열의 인덱스와 그 값을 출력
	        System.out.println("입력된 숫자와 그 인덱스:");
	        for (int i = 0; i < 10; i++) {
	            System.out.println("인덱스 " + i + ": 값 " + num[i]);
	        }
	    }
	}

