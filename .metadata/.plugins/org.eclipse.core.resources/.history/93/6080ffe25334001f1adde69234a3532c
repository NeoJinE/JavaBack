package Ch06_class_sec17;

public class Carculator {
	//static 필드 선언 및 초기화
	static double pi = 3.141592;
	
	//인스턴스 멤버 필드
	int a ;
	
	//static 메서드
	static int plus(int x, int y) {
		return x + y;
	}
	
	static int minus(int x, int y) {
		return x - y;
	}
	
	//인스턴스 메서드
	public void sum() {
		System.out.println();
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
}
