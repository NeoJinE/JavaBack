package sec10;

import java.util.Scanner;

public class Rectangle {

	//1.멤버변수선언
	int width;
	int height;
	
	//2.메서드1작성(매개변수로 가로,세로길이 입력받기)
	public void  setRectangle(int width,int height) {
		width=width;
		height=height;
	}
		
	//3.메서드2작성(넓이를 구해 출력)
	public void  showRectangle() {
		System.out.println("넓이: " + width * height);
	}
	
	
	
	
	
	
	
	
	
	
	
}
