package Ch07_inheritance.sec02_1;

public class Student extends Person {

	//1.멤버변수 선언
	int stdNo;
	
	//2.
	public void setstdNo() {
		setPerson();
		Name = "홍길동";
		Ssn = "123456-1234567";
		stdNo = 2024001;
	}

	//3.
	@Override
	public String toString() {
		return Name + Ssn +" "+ stdNo;
	}
	
	
	
}
