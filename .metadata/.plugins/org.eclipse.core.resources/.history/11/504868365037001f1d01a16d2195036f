package Ch07_inheritance.sec05;

public class Person {

	//1.멤버변수 선언
		//1.멤버변수선언
		private String Name,Ssn;
		 
		//2.(멤버메서드)생성자 선언
		
		public void setPerson(String Name, String Ssn) {
			this.Name = Name;
			this.Ssn = Ssn;
		}

		//3.toString 작성
		@Override
		public String toString() {
		//두 멤버 필드값을 문자열로 반환.	
			return "이름: " + Name + " " + "주민번호: " + Ssn;
		}
	}
	
	
	

