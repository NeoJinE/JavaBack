package sec25T.main;

//import ch06_class.sec25.prd ; //패키지명만 쓰면 오류가 발생, 클래스명까지 명시

import ch06_class.sec25.prd.Product; //Product 클래스만 import
import ch06_class.sec25.std.*;// * 기호 이용해서 모든 클래스 라고 명시
//ch06_class.sec25.std 패키지 내 모든 클래스가 import


public class AppMain {

	public static void main(String[] args) {
		// 다른 패키지에 있는 클래스 사용
		// import 해야 함
		Product p = new Product("1001", "노트북", "삼성", 500000, 10);
		Student s = new Student();
		
		System.out.println(p);
		
	}

}
