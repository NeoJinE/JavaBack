package Ch07_inheritance.sec03;
//상속에서의 생성자 호출
class A {
	//기본생성자
	public A() {
		System.out.println("기본 생성자 A");
	}
}

class B extends A{
	//기본 생성자
	public B() {
		System.out.println("기본 생성자 B");
	}
}
public class ConstructorTest1 {

	public static void main(String[] args) {
		// 생성자 실행
		B b = new B(); //1.기본 생성자 함수 호출
		
		

	}

}
