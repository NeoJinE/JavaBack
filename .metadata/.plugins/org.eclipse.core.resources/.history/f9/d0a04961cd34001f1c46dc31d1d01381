package Ch06_class_sec16;

public class BookMain {

	public static void main(String[] args) {
		// 여러생성자 사용 예제
		Book b1 = new Book();//기본 생성자 메서드에서 매개변수가 2개인 생성자가 호출되었다.
		Book b2 = new Book("1001","자바");// 매개변수가 2개인 생성자 호출
		Book b3 = new Book("1001","자바"," 스크립트",22000);// 매개변수가 4개인 생성자 호출
		
		
		
		
		
		
		b1.showBookInfo(); 
		System.out.println();
		b2.showBookInfo();
		System.out.println();
		b3.showBookInfo();
		System.out.println();
		

	}

}
