package ch02;

public class Ch02_12_VariableEx3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String Pen = "볼펜";
		String Pencil = "연필";
		String Eraser = "지우개";
		
		
		int PenPrice = 1000;
		int PencilPrice = 300;
		int EraserPrice = 500;
		int totalPrice = Pen + Pencil + Eraser;
		
		System.out.println("상품: "+ Pen + "가격: "+ PenPrice);
		System.out.println("상품: "+ Pencil + "가격: "+ PencilPrice);
		System.out.println("상품: "+ Eraser + "가격: "+ EraserPrice);
		System.out.println("총가격: "+ totalPrice);
		
		
		
	}

}
